{
    "family": "fiverr-api",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::340752808368:role/fiverr-execution-role",
    "containerDefinitions": [
        {
            "name": "fiverr-api",
            "image": "340752808368.dkr.ecr.ap-south-1.amazonaws.com/fiverr-backend:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "MONGODB_URI",
                    "value": "mongodb+srv://fiverrdev:fiverrclonedev@fiverrrclone.08b9o.mongodb.net/fiverr?retryWrites=true&w=majority&appName=FiverrrClone"
                },
                {
                    "name": "JWT_KEY",
                    "value": "e13743a7f1db7f4246badd6fd6ff54ff"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                },
                {
                    "name": "CORS_ORIGIN",
                    "value": "https://www.fiverr.publicvm.com"
                },
                {
                    "name": "CLIENT_URL",
                    "value": "https://fiverrservices.vercel.app"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/fiverr-api",
